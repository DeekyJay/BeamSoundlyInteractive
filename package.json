    "history": "^2.0.0",
    "howler": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.10.0",
    "moment": "^2.13.0",
    "normalizr": "^2.0.1",
    "proto-loader": "^0.2.0",
    "protobufjs": "^5.0.1",
    "rc-slider": "^5.2.1",
